{
  "name": "csgo-rcon-nodejs",
  "version": "0.0.0",
  "description": "NPM packages for csgo-rcon-nodejs",
  "private": true,
  "scripts": {
    "start": "cd ./app && node ./server.js",
    "dev": "cd ./app && nodemon -L --ignore csgo-rcon.json --watch ./ ./server.js",
    "webpack": "webpack --watch --watch-poll --mode development --config ./_scripts/webpack/webpack.config.js",
    "webpack:production": "webpack --mode production --config ./_scripts/webpack/webpack.config.js",
    "lint": "eslint -c ./package.json ./"
  },
  "author": "Glenn de Haan",
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "no-console": 0,
      "react/prop-types": 0,
      "comma-dangle": [
        "error",
        "never"
      ],
      "indent": [
        "error",
        4
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "settings": {
      "react": {
        "pragma": "h",
        "version": "16.0"
      }
    }
  },
  "eslintIgnore": [
    "public/dist"
  ],
  "dependencies": {
    "atob": "^2.1.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.1.3",
    "btoa": "^1.2.1",
    "css-loader": "^0.28.11",
    "ejs": "^2.6.1",
    "express": "^4.16.3",
    "express-ws": "^4.0.0",
    "mini-css-extract-plugin": "^0.4.1",
    "node-json-db": "^0.9.0",
    "node-sass": "^4.9.1",
    "preact": "^8.3.1",
    "preact-router": "^2.6.1",
    "sass-loader": "^7.0.3",
    "simple-node-logger": "^0.93.40",
    "socket.io": "^2.1.1",
    "sockette": "^2.0.0",
    "srcds-rcon": "github:randunel/node-srcds-rcon",
    "uuid": "^3.3.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-manifest-plugin": "^2.0.3"
  },
  "devDependencies": {
    "nodemon": "^1.17.3",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-react": "^7.10.0"
  }
}
